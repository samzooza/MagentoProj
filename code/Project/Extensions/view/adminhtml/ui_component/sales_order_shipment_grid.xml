<?xml version="1.0" encoding="UTF-8"?>
<listing xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Ui:etc/ui_configuration.xsd">
    <listingToolbar name="listing_top">
        <massaction name="listing_massaction">
            <action name="order_printshippinglabel">
                <argument name="data" xsi:type="array">
                    <item name="config" xsi:type="array">
                        <item name="type" xsi:type="string">order_printshippinglabel</item>
                        <item name="label" xsi:type="string" translate="true">SCG Express: Print Shipping Labels</item>
                        <item name="url" xsi:type="url" path="extensions/order/massPrintShippingLabel"/>
                        <item name="confirm" xsi:type="array">
                            <item name="title" xsi:type="string" translate="true">Comfirm Print Shipping Label</item>
                             <item name="message" xsi:type="string" translate="true">Are you sure print shipping order(s)?</item>
                        </item>
                    </item>
                </argument>
            </action>
        </massaction>
    </listingToolbar>
</listing>